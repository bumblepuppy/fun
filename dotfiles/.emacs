;; disabling keybinds of ergoemacs and cua mode
(ergoemacs-mode 0)
(cua-mode 0)

;; do not make backup files
 (setq make-backup-files nil)

;; Make our default background gray
(add-to-list 'default-frame-alist '(background-color . "lightgray"))



